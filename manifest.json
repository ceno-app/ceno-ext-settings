{

  "description": "Adds a toolbar button for CENO settings page.",
  "manifest_version": 2,
  "name": "CENO Settings",
  "version": "1.0.41",
  "icons": {
    "48": "icons/ceno-logo-48.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "settings.ceno@equalit.ie"
      }
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icons/ceno-logo-32.png",
    "default_title": "CENO",
    "browser_style": true
  },

  "options_ui": {
    "page": "index.html",
    "browser_style": true
  },

  "background": {
    "scripts": ["button.js", "modifyheaders.js"]
  },

  "incognito": "spanning"
}
